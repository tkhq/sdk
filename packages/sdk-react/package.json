    "@turnkey/crypto": "workspace:*",
    "@turnkey/sdk-server": "workspace:*",
    "usehooks-ts": "^3.1.0",
    "libphonenumber-js": "^1.11.14",
    "next": "^15.0.2",
    "react-apple-login": "^1.1.6",
    "react-international-phone": "^4.3.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
