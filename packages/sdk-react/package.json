{
  "name": "@turnkey/sdk-react",
  "version": "2.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./styles": {
      "import": "./dist/styles.esm.css",
      "require": "./dist/styles.cjs.css",
      "default": "./dist/styles.esm.css"
    }
  },
  "types": "./dist/index.d.ts",
  "license": "Apache-2.0",
  "description": "React SDK",
  "keywords": [
    "Turnkey"
  ],
  "author": {
    "name": "Turnkey",
    "url": "https://turnkey.com"
  },
  "homepage": "https://github.com/tkhq/sdk",
  "bugs": {
    "url": "https://github.com/tkhq/sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tkhq/sdk.git",
    "directory": "packages/sdk-react"
  },
  "files": [
    "dist/",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./dist ./.cache",
    "typecheck": "tsc -p tsconfig.typecheck.json"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
