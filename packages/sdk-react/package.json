{
  "name": "@turnkey/sdk-react",
  "version": "5.4.0-beta.6",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./styles": {
      "import": "./dist/styles.esm.css",
      "require": "./dist/styles.cjs.css",
      "default": "./dist/styles.esm.css"
    }
  },
  "types": "./dist/index.d.ts",
  "license": "Apache-2.0",
  "description": "React SDK",
  "keywords": [
    "Turnkey"
  ],
  "author": {
    "name": "Turnkey",
    "url": "https://turnkey.com"
  },
  "homepage": "https://github.com/tkhq/sdk",
  "bugs": {
    "url": "https://github.com/tkhq/sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkhq/sdk.git",
    "directory": "packages/sdk-react"
  },
  "files": [
    "dist/",
    "CHANGELOG.md",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./dist ./.cache",
    "typecheck": "tsc -p tsconfig.typecheck.json",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "dependencies": {
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@icons-pack/react-simple-icons": "13.7.0",
    "@mui/icons-material": "7.3.2",
    "@mui/material": "7.3.2",
    "@noble/hashes": "1.8.0",
    "@react-oauth/google": "0.12.1",
    "@turnkey/crypto": "workspace:*",
    "@turnkey/sdk-browser": "workspace:*",
    "@turnkey/sdk-server": "workspace:*",
    "@turnkey/sdk-types": "workspace:*",
    "@turnkey/wallet-stamper": "workspace:*",
    "jwt-decode": "4.0.0",
    "libphonenumber-js": "1.12.15",
    "next": "15.2.3",
    "react-apple-login": "1.1.6",
    "react-international-phone": "4.6.0",
    "usehooks-ts": "3.1.1"
  },
  "peerDependencies": {
    "@types/react": ">=18.2.0 <20",
    "react": "^18.3.1 || ^19.0.0",
    "react-dom": "^18.3.1 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "postcss-import": "16.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
