{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "noEmit": true,
    "jsx": "react-jsx",
    "tsBuildInfoFile": "./.cache/.typecheck.tsbuildinfo",
    "target": "ES2022",
    "module": "preserve",
    "moduleResolution": "bundler",
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.js",
    "src/**/*.jsx",
    "src/**/*.json"
  ]
}
