{
  "name": "@turnkey/gas-station",
  "version": "3.1.1",
  "description": "Turnkey Gas Station SDK for EIP-7702 delegated execution",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "author": {
    "name": "Turnkey",
    "url": "https://turnkey.com/"
  },
  "homepage": "https://github.com/tkhq/sdk",
  "bugs": {
    "url": "https://github.com/tkhq/sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkhq/sdk.git",
    "directory": "packages/gas-station"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c ../../rollup.config.base.mjs",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf ./dist ./.cache"
  },
  "keywords": [
    "turnkey",
    "gas-station",
