  "dependencies": {
    "viem": "^2.34.0"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@turnkey/sdk-server": "workspace:*",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "viem": "^2.24.0",
    "@turnkey/sdk-server": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
