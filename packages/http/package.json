{
  "name": "@turnkey/http",
  "version": "2.16.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "license": "Apache-2.0",
  "description": "Typed HTTP client for interacting with Turnkey API",
  "keywords": [
    "Turnkey"
  ],
  "author": {
    "name": "Turnkey",
    "url": "https://turnkey.com/"
  },
  "homepage": "https://github.com/tkhq/sdk",
  "bugs": {
    "url": "https://github.com/tkhq/sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tkhq/sdk.git",
    "directory": "packages/http"
  },
  "files": [
    "dist/",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "version": "node -p \"'export const VERSION = ' + JSON.stringify(require('./package.json').name + '@' + require('./package.json').version) + ';'\" > src/version.ts",
    "prepublishOnly": "pnpm -w run clean-all && pnpm -w run build-all",
    "build": "rollup -c",
    "clean": "rimraf ./dist ./.cache",
    "test": "jest",
    "typecheck": "tsc -p tsconfig.typecheck.json"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "@turnkey/api-key-stamper": "workspace:*",
    "@turnkey/encoding": "workspace:*",
    "@turnkey/webauthn-stamper": "workspace:*"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
