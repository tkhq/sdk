{
  "entryPoints": ["src/index.ts"],
  "excludeInternal": true,
  "includeVersion": true,
  "fileExtension": ".mdx",
  "plugin": ["typedoc-plugin-markdown"],
  "projectDocuments": ["documents/*.md"],
  "outputs": [
    {
      "name": "html",
      "path": "./docs/html"
    },
    {
      // requires typedoc-plugin-markdown
      "name": "markdown",
      "path": "./docs/markdown",
      "fileExtension": ".mdx"
    }
  ]
}
