  },
  "homepage": "https://github.com/tkhq/sdk/packages/sdk-types#readme",
  "bugs": {
    "url": "https://github.com/tkhq/sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkhq/sdk.git",
    "directory": "packages/sdk-types"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript": "5.4.3"
  },
  "engines": {
    "node": ">=18.0.0"
