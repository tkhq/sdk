      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    }
  },
  "license": "Apache-2.0",
  "description": "Turnkey SDK Types",
  "keywords": [
    "Turnkey"
  ],
  "author": {
    "name": "Turnkey",
