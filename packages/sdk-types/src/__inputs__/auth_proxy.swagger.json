        "publicKey": {
          "type": "string",
          "description": "Client-side public key generated by the user, which will be added to the JWT response and verified in subsequent requests via a client proof signature"
        }
      },
      "required": ["otpId", "otpCode"]
    },
    "v1VerifyOtpResponse": {
