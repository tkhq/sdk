{
  "entryPoints": ["src/providers/client/Types.tsx"],
  "name": "react-wallet-kit",
  "exclude": [
    "src/utils.ts",
    "**/assets/**",
    "**/components/**",
    "**/node_modules/**"
  ],
  "excludeInternal": true,
  "includeVersion": true,
  // "fileExtension": "json",
  "alwaysCreateEntryPointModule": false,
  "gitRevision": "main",
  "router": "structure-dir",
  // "plugin": ["typedoc-plugin-markdown"],
  "projectDocuments": ["documents/*.md", "CHANGELOG.md"],
  // "expandParameters": true,
  "validation": {
    "notExported": false,
    "notDocumented": false
  },
  "outputs": [
    {
      // requires typedoc-plugin-markdown
      "name": "json",
      "path": "./generated-docs/sdks",
      "fileExtension": "json"
    }
  ]
}
