    "@rollup/plugin-strip": "^3.0.4",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.0.1",
    "glob": "^8.0.3",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^4.1.10",
    "typescript": "5.4.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
