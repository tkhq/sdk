{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "noEmit": true,
    "tsBuildInfoFile": "./.cache/.typecheck.tsbuildinfo",
    "resolveJsonModule": true,
    "ignoreDeprecations": "5.0",
    "target": "ES2022",
    "module": "esnext",
    "moduleResolution": "node",
