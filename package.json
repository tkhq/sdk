  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@jest/globals": "^29.3.1",
    "@jest/types": "^29.3.1",
    "@tsconfig/node16-strictest": "^1.0.4",
    "@turnkey/jest-config": "workspace:*",
    "@types/node": "^16.0.0",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "packageManager": "pnpm@7.13.3"
