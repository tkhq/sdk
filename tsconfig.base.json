{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "ignoreDeprecations": "5.0",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "incremental": true,
    "isolatedModules": true,
    "noPropertyAccessFromIndexSignature": false,
    "resolveJsonModule": true,
    "sourceMap": true
  }
}
