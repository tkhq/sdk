{
  "entryPoints": ["packages/*"],
  "entryPointStrategy": "packages",
  "includeVersion": true,
  "excludeExternals": true,
  "alwaysCreateEntryPointModule": true,
  "gitRevision": "main",
  "router": "structure-dir",
  "outputs": [
    {
      "name": "json",
      "path": "./generated-docs/sdks.json",
      "fileExtension": ".json"
    }
  ],
  "hideGenerator": true,
  "headings": {
    "readme": false,
    "document": false
  }
}
