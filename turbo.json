{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
      "build": {
        "inputs": ["$TURBO_DEFAULT$", ".env*"],
        "dependsOn": ["^build"],
        "outputs": [
          "build/**",
          ".vercel/**",
          "dist/**",
          ".next/**",
          "!.next/cache/**"
        ]
      },
      "clean": {
        "dependsOn": ["^clean"]
      },
      "dev": {
        "persistent": true,
        "cache": false
      }
    }
  }